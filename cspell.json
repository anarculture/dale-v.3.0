  "ignoreRegExpList": [
    // URLs
    "https?://[^\\s]+",
    "http://[^\\s]+",
    
    // File paths
    "['\\\"][^'\\\"]*\\.(js|ts|tsx|jsx|json|md|yml|yaml)['\\\"]",
    
    // Code comments
    "//.*",
    "/\\*.*\\*/",
    
    // HTML attributes
    "class=['\\\"][^'\\\"]*['\\\"]",
    "id=['\\\"][^'\\\"]*['\\\"]",
    
    // Console logs and debug
    "console\\.log\\([^)]+\\)",
    "console\\.error\\([^)]+\\)",
    "console\\.warn\\([^)]+\\)",
    
    // String templates
    "`[^`]*`",
    
    // Environment variables
    "\\$\\{[^}]+\\}",
    
    // Imports/exports
    "from ['\\\"][^'\\\"]*['\\\"]",
    "import .* from ['\\\"][^'\\\"]*['\\\"]"
  ],
  "languageSettings": [
    {
      "languageId": "typescript",
      "ignoreRegExpList": [
        "import.*from.*['\\\"][^'\\\"]*['\\\"]"
      ]
    },
    {
      "languageId": "javascript",
      "ignoreRegExpList": [
        "import.*from.*['\\\"][^'\\\"]*['\\\"]"
      ]
    },
    {
      "languageId": "python",
      "ignoreRegExpList": [
        "from .* import .*",
        "import .*"
      ]
    }
  ]
